{
  "name": "avocado_desktop",
  "homepage": "./",
  "main": "public/electron.js",
  "version": "0.1.0",
  "private": true,
  "author": {
    "name": "cyber doge",
    "email": "tagirman998@gmail.com"
  },
  "dependencies": {
    "@ant-design/icons": "^4.5.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "antd": "^4.12.3",
    "classnames": "^2.2.6",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "electron-is-dev": "^1.2.0",
    "mobx": "^6.1.6",
    "mobx-react-lite": "^3.2.0",
    "node-sass": "^5.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.2",
    "slash": "^3.0.0",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test --env=jsdom",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder",
    "release": "yarn react-build && electron-builder --publish=always",
    "build": "yarn react-build && yarn electron-build",
    "start": "concurrently \"cross-env BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && electron bootwithbabel.js . --inspect=9223 --remote-debugging-port=9222\"",
    "electron-start": "electron ."
  },
  "build": {
    "appId": "io.cyber_doge.avocado",
    "mac": {
      "category": "public.app-category.entertainment",
      "target": [
        "zip",
        "dmg"
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "public/icon.png"
    },
    "win": {
      "target": "nsis",
      "icon": "public/icon.png"
    },
    "publish": {
      "provider": "github"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/register": "^7.12.13",
    "electron": "^11.2.3",
    "electron-builder": "^22.9.1",
    "prettier": "^2.2.1",
    "wait-on": "^5.2.1"
  }
}
